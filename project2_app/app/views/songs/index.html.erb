<h2>Create a post!</h2>
<h4>Select a song below.</h4>
<div id="index-container">
	<div class="row">
		<div class="small-12 large-12">
			<% @songs.each do |song| %>
				<div class="song-index-box">
					<div>
						
						<%= link_to song_path(song.id) do %>
							<%= song.name %> by <%= song.artists.first.name %> 
						<% end %>
					</div>
				</div>
		 	<% end %>
	 	</div>
	</div>
</div>